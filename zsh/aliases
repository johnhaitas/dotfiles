#alias ls='ls --color=auto'

if [ -f /usr/local/mysql/bin/mysql ]; then alias mysql=/usr/local/mysql/bin/mysql; fi
if [ -f /usr/local/mysql/bin/mysqladmin ]; then alias mysqladmin=/usr/local/mysql/bin/mysqladmin; fi

if command -v gcloud > /dev/null 2>&1; then alias gc=gcloud; fi
if command -v kubectl > /dev/null 2>&1; then alias kc=kubectl; fi
